<%= simple_form_for @health_history, url: client_health_histories_path do |f| %>
  <%= f.input :medical_condition, as: :radio_buttons,
    collection: [['Acne', 'acne'], ['Arthritis', 'arthritis']],
    label_method: :second, value_method: :first %>
  <%= f.submit 'Submit', class: 'btn btn-primary' %>
<% end %>
